ti = -Wall -Wextra

maintest :maintest.o fonction.o fonctiontest.o
	gcc $^ -o $@ $(ti)

maintest.o: maintest.c fonction.h fonctiontest.h
	gcc -c $< -o $@ $(ti)

fonction.o : fonction.c fonction.h
	gcc -c $< -o $@ $(ti)

fonctiontest.o:fonctiontest.c fonctiontest.h 
	gcc -c $< -o $@ $(ti)

clean :
	rm -f maintest *.o